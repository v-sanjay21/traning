
1.Is N an exact multiple of M ?

import java.io.*;
import java.util.*;
class UserMainCode
{
public int isMultiplel(int input1,int input2){
  int val=0;
  if(input1==0 || input2==0) val=8;
  else if((input1%input2)!=0) val=1;
  else val=2;
  return val;
   }
}

2.Of given 5 numbers how many are even ?

#include<stdio.h>
#include<string.h>
int countEvens(int input1,int input2,int input3, int input4,int input5)
{
  int cnt=0;
  if(input1<0) input1=(-1)*input1;
  if(input2<0) input2=(-1)*input2;
  if(input3<0) input3=(-1)*input3;
  if(input4<0) input4=(-1)*input4;
  if(input5<0) input5=(-1)*input5;
  if(input1%2==0) cnt++;
  if(input2%2==0) cnt++;
  if(input3%2==0) cnt++;
  if(input4%2==0) cnt++;
  if(input5%2==0) cnt++;
    return cnt;
}

3.Of given 5 numbers how many are odd ?

import java.io.*;
import java.util.*;
class UserMainCode
{
  public int countEvensint input,int input2,int input3,int input4,int inputs){
    int cnt=0;
   if(input1<0) input1=(-1)*input1;
   if(input2<0) input2=(-1)*input2; 
   if(input3<0) input3=(-1)*input3;
   if(input4<0) input4=(-1)*input4;
   if(input5<0) input5=(-1)*input5;
   if(input1%2!=0) cnt++; 
   if(input2%2!=0) cnt++; 
   if(input3%2!=0) cnt++; 
   if(input4%2!=0) cnt++; 
   if(input5%2!=0) cnt++;
  return cnt;
   }
}

4.Of given 5 numbers how many are even or odd ?

import java.io.*;
import java.util.*;
class UserMaincode
{
  public int countEvensOdds(int input, int input2 int inputs, int input4, int inputs,String
input6){
    int cnt=0;
       if(input6.equalsIgnoreCase("odd"))
{
       if(input1<0) input1=(-1)*input1;
       if(input2<0) input2=(-1)*input2;
       if(input3<0) input3=(-1)*input3;
       if(input4<0) input4=(-1)*input4;
       if(input5<0) input5=(-1)*input5,
       if(input1%2!=0) cnt++;
       if(input2%2!=0) cnt++;
       if(input3%2!=0) cnt++;
       if(input4%2!=0) cnt++;
       if(input5%2!=0) cnt++;
}
     else if(input6.equalsIgnoreCase("even"))
{
     if(input1<0) input1=-1)*input1;
     if(input2<0) input2=(-1)*input2;
     if(input3<0) input3=(-1)*input3;
     if(input4<0) input4-(-1)*input4; 
     if(input5<0) input5=(-1)*input5;
     if(input1%2==0) cnt++;
     if(input2%2==0) cnt++;
     if(input3%2==0) cnt++;
     if(input4%2==0) cnt++;
     if(input5%2==0) cnt++;
  }
     return cnt;
}
}
